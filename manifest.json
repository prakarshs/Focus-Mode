{
    "manifest_version": 3,
    "name": "Focus Mode",
    "version": "1.0",
    "description": "Remove Recommended Videos From Youtube When You Need To Focus !",
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "images/icon16.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
      }
    },
    "icons": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    },
    "content_scripts": [
      {
        "js": ["content.js"],
        "all_frames": true,
      "matches": ["http://*/*", "https://*/*", "file:///*"],
      "match_about_blank": true
      }
    ]
  }
  